<Window x:Class="WPFProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="cmdStartEngine" Content="Start Engine" HorizontalAlignment="Left" Margin="198,40,0,0" VerticalAlignment="Top" Width="89" Click="cmdStartEngine_Click"/>
        <Button x:Name="cmdDrive" Content="Drive" HorizontalAlignment="Left" Margin="198,92,0,0" VerticalAlignment="Top" Width="89" Click="cmdDrive_Click"/>
        <Button x:Name="cmdPark" Content="Park" HorizontalAlignment="Left" Margin="198,141,0,0" VerticalAlignment="Top" Width="89" Click="cmdPark_Click"/>
        <Button x:Name="cmdReverse" Content="Reverse" HorizontalAlignment="Left" Margin="198,187,0,0" VerticalAlignment="Top" Width="89" Click="cmdReverse_Click"/>
        <Button x:Name="cmdTurnOff" Content="Turn Off" HorizontalAlignment="Left" Margin="198,235,0,0" VerticalAlignment="Top" Width="89" Click="cmdTurnOff_Click"/>
    </Grid>
</Window>
